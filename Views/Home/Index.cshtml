@{ ViewBag.Title = "Home Page"; }


@{ ViewBag.Title = "Index"; }

<link href="~/Content/StyleSheet1.css" rel="stylesheet" />
<script src="~/Scripts/JavaScriptS.js"></script>
<!-- Slideshow container -->
<div class="slideshow-container" data-delay="5000">

    <!-- Full-width images with number and caption text -->
    <div class="mySlides fade">
        <div class="numbertext">1 / 3</div>
        <img src="https://media.gettyimages.com/photos/empty-chairs-in-front-of-mirrors-in-hair-cut-shop-at-tokyo-picture-id1132679098?k=6&m=1132679098&s=612x612&w=0&h=Fig2V7BN7GHA5OoFOCF52U8XsJzEgQsy_SSYDpI3orU=" style="width:100%">
        <div class="text">Caption Text</div>
    </div>

    <div class="mySlides fade">
        <div class="numbertext">2 / 3</div>
        <img src="https://images.unsplash.com/photo-1560066984-138dadb4c035?ixid=MnwxMjA3fDB8MHxzZWFyY2h8Mnx8YmVhdXR5JTIwc2Fsb258ZW58MHx8MHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=600&q=60" style="width:100%">
        <div class="text">Caption Two</div>
    </div>

    <div class="mySlides fade">
        <div class="numbertext">3 / 3</div>
        <img src="https://images.unsplash.com/photo-1540555700478-4be289fbecef?ixid=MnwxMjA3fDB8MHxzZWFyY2h8M3x8YmVhdXR5JTIwc2Fsb258ZW58MHx8MHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=600&q=60" style="width:100%">
        <div class="text">Caption Three</div>
    </div>

    <!-- Next and previous buttons -->
    <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
    <a class="next" onclick="plusSlides(1)">&#10095;</a>
</div>
<br>

 @*The dots/circles*@ 
<div style="text-align:center">
    <span class="dot" onclick=currentSlide(1)></span>
    <span class="dot" onclick=currentSlide(2)></span>
    <span class="dot" onclick=currentSlide(3)></span>
</div>






